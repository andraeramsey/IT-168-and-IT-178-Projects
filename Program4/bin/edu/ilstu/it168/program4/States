package edu.ilstu;

import java.util.Scanner;

public class Menu {

   /**
    * <insert method description here>
    *
    * @param:
    *
    */
   public static void main(String[] args) {
       int choice = 0;
       Scanner scannerObject = new Scanner(System.in);

       System.out.println("Redbird Merchandise Shop\n");
       System.out.println("1:  Redbird T-shirt");
       System.out.println("2:  Redbird Sweatshirt");
       System.out.println("3:  Redbird Hat");
       System.out.println("4:  Redbird Banner");
       System.out.println("5.  Done.\n");

       System.out.print("Make your selection (1-5) ");
       choice = scannerObject.nextInt();

       while (choice != 4){
           switch (choice){
               case 1:
                   System.out.println("How many of these would you like?");
                   break;
               case 2:
                   System.out.println("Choice was 2");
                   break;
               case 3:
                   System.out.println("Choice was 3");
                   break;
               default:
                   System.out.println("Invalid choice, please enter 1-5.");
           }

           System.out.println("Menu");
           System.out.println("1.  Choice 1");
           System.out.println("2.  Choice 2");
           System.out.println("3.  Choice 3");
           System.out.println("4.  Quit");

           System.out.print("Please enter your menu choice: ");
           choice = scannerObject.nextInt();
       }

       System.out.println("Out of loop");
   }

}


public void readCustomer(AddressBook customer)
